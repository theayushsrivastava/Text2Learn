<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.c2mtechnology.text2learn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".learn.TestActivity"></activity>
        <activity android:name=".AllFilesActivity" />
        <activity android:name=".learn.LearnActivity" />
        <activity android:name=".MakeFileActivity" />
